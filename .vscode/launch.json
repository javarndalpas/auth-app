{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Backend",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/backend/index.js",
        "envFile": "${workspaceFolder}/backend/.env",
        "env": {
          "NODE_ENV": "development",
          "PORT": "5000",
          "JWT_SECRET": "your_jwt_secret",
          "MONGO_URI": "mongodb://localhost:27017/propertiesDB"
        }
      }
    ]
  }
  